@model Outercurve.Projects.ViewModels.CLAIndexViewModel
           
@{
    Layout.Title = "CLA Viewing";
}

<h1> CLA Viewing</h1>

<h2>Projects</h2>



    @foreach (var i in Model.Projects) {
